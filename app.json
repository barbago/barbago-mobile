{
  "expo": {
    "name": "Barbago Mobile",
    "slug": "barbago-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "barbago",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.barbagoapp.mobile",
      "supportsTablet": true,
      "usesAppleSignIn": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "package": "com.barbagoapp.mobile"
    },
    "web": {
      "build": {
        "babel": {
          "include": ["react-leaflet"]
        }
      },
      "favicon": "./src/assets/images/favicon.png"
    },
    "description": "",
    "githubUrl": "https://github.com/barbago/barago-mobile"
  }
}
